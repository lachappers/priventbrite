<p style="color: green"><%= notice %></p>
<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<%= link_to "New Event", new_event_path %>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Location</th>
      <th>Host</th>
      <th>Attendees</th>
      <th>Title</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody id = "events">
    <% @events.each do |event|%>
      <tr>
        <td><%= event.date %></td>
        <td><%= event.location %></td>
        <td><%= event.creator.username %></td>
        <td><%= event.rsvps.count %></td>
        <td><%= link_to event.title, event %></td>
        <td><%= event.description %></td>
      </tr>
      <%end%>
  </tbody>
</table>
